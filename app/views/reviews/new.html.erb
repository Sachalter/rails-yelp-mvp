<h1>Reviews#new</h1>
<p>Find me in app/views/reviews/new.html.erb</p>

<h1>Create a new review</h1>

<%= simple_form_for([ @restaurant, @review ]) do |f| %>
  <%= f.input :content, placeholder:  "give me a content" %>
  <%= f.input :rating, collection: 0..5, placeholder:  "rate from 0 to 5" %>
  <%= f.submit%>
<%end%>
